    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-typography": "^2.2.7",
    "gatsby-source-filesystem": "^2.1.46",
