{
  "name": "cyberworks",
  "private": true,
  "description": "business website",
  "version": "1.0.0",
  "author": "olonnye taylor (olonnye.com)",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "build-netlify": "npm run develop-prod",
    "start": "npm run develop-dev",
    "develop-dev": "gatsby develop -o",
    "develop-prod": "DEPLOY_ENV=lbn_published_production gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "tslint --project .",
    "test": "start-server-and-test ciDev http://localhost:8000 cy:open",
    "ci": "start-server-and-test ciDev http://localhost:8000 cy:run",
    "ciDev": "gatsby develop",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "type-check": "tsc --noEmit",
    "upgrade": "npm-check --update",
    "upgrade-interactive": "npm-check --update"
  },
  "browerslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
